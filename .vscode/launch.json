{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Build",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\script\\build.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Update",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}\\script\\update.js",
            "outFiles": [
                "${workspaceFolder}/**/*.js"
            ]
        }
    ]
}